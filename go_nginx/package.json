{
  "name":            "routify-starter",
  "private":         true,
  "scripts":         {
    "dev":           "run-p dev:*",
    "dev:routify":   "routify",
    "dev:vite":      "vite",
    "preview":       "vite preview",
    "build":         "run-s build:*",
    "build:routify": "routify -b",
    "build:vite":    "vite build",
    "watch":         "run-p watch:*",
    "watch:routify": "routify",
    "watch:vite":    "vite build --watch --mode development"
  },
  "devDependencies": {
    "@roxi/routify":                "^2.18.8",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "autoprefixer":                 "^10.4.7",
    "npm-run-all":                  "^4.1.5",
    "postcss":                      "^8.4.14",
    "postcss-import":               "^14.1.0",
    "sass":                         "^1.57.1",
    "svelte":                       "^3.50.0",
    "svelte-preprocess":            "^4.10.7",
    "vite":                         "^3.1.0"
  },
  "dependencies":    {
    "normalize.css": "^8.0.1",
    "qs":            "^6.11.0"
  }
}
